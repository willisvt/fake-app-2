        active: true,\r\n        },\r\n      ],\r\n      fields: [{ id: 1 }],\r\n      phoneData: [{ id: 1 }],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    /**\r\n     * Push the row in form\r\n     */\r\n    AddformData() {\r\n      this.fields.push({ name: \"\", email: \"\", subject: \"\", message: \"\" });\r\n    },\r\n    /**\r\n     * Delete the row\r\n     */\r\n    deleteRow(index) {\r\n      if (confirm(\"Are you sure you want to delete this element?\"))\r\n        this.fields.splice(index, 1);\r\n    },\r\n    /**\r\n     * Add the phone number in form\r\n     */\r\n    AddPhoneNo() {\r\n      this.phoneData.push({ phone: \"\" });\r\n    },\r\n    /**\r\n     * Delete the row from form\r\n     */\r\n    deletePhone(index) {\r\n      this.phoneData.splice(index, 1);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    <div class=\"row\">\r\n      <div class=\"col-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Example</h4>\r\n            <form class=\"repeater\" enctype=\"multipart/form-data\">\r\n              <div>\r\n                <div\r\n                  v-for=\"(field, index) in fields\"\r\n                  :key=\"field.id\"\r\n                  class=\"row\"\r\n                >\r\n                  <div class=\"mb-3 col-lg-2\">\r\n                    <label for=\"name\">Name</label>\r\n                    <input\r\n                      id=\"name\"\r\n                      v-model=\"field.name\"\r\n                      type=\"text\"\r\n                      name=\"untyped-input\"\r\n                      class=\"form-control\"\r\n                    />\r\n                  </div>\r\n\r\n                  <div class=\"mb-3 col-lg-2\">\r\n                    <label for=\"email\">Email</label>\r\n                    <input\r\n                      id=\"email\"\r\n                      v-model=\"field.email\"\r\n                      type=\"email\"\r\n                      class=\"form-control\"\r\n                    />\r\n                  </div>\r\n\r\n                  <div class=\"mb-3 col-lg-2\">\r\n                    <label for=\"subject\">Subject</label>\r\n                    <input\r\n                      id=\"subject\"\r\n                      v-model=\"field.subject\"\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                    />\r\n                  </div>\r\n\r\n                  <div class=\"mb-3 col-lg-2\">\r\n                    <label for=\"resume\">Resume</label>\r\n                    <input id=\"resume\" type=\"file\" class=\"form-control-file\" />\r\n                  </div>\r\n\r\n                  <div class=\"mb-3 col-lg-2\">\r\n                    <label for=\"message\">Message</label>\r\n                    <textarea\r\n                      id=\"message\"\r\n                      v-model=\"field.message\"\r\n                      class=\"form-control\"\r\n                    ></textarea>\r\n                  </div>\r\n\r\n                  <div class=\"col-lg-2 align-self-center\">\r\n                    <div class=\"d-grid\">\r\n                      <input\r\n                        type=\"button\"\r\n                        class=\"btn btn-primary btn-block\"\r\n                        value=\"Delete\"\r\n                        @click=\"deleteRow(index)\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <input\r\n                type=\"button\"\r\n                class=\"btn btn-success mt-3 mt-lg-0\"\r\n                value=\"Add\"\r\n                @click=\"AddformData\"\r\n              />\r\n            </form>\r\n          </div>\r\n          <!-- end card-body -->\r\n        </div>\r\n        <!-- end card -->\r\n      </div>\r\n      <!-- end col -->\r\n    </div>\r\n    <!-- end row -->\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Nested</h4>\r\n            <form class=\"outer-repeater\">\r\n              <div class=\"outer\">\r\n                <div class=\"outer\">\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"formname\">Name :</label>\r\n                    <input\r\n                      id=\"formname\"\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      placeholder=\"Enter your Name...\"\r\n                    />\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"formemail\">Email :</label>\r\n                    <input\r\n                      id=\"formemail\"\r\n                      type=\"email\"\r\n                      class=\"form-control\"\r\n                      placeholder=\"Enter your Email...\"\r\n                    />\r\n                  </div>\r\n\r\n                  <div class=\"inner-repeater mb-4\">\r\n                    <div class=\"inner mb-3\">\r\n                      <label>Phone no :</label>\r\n                      <div\r\n                        v-for=\"(data, index) in phoneData\"\r\n                        :key=\"data.id\"\r\n                        class=\"inner mb-3 row\"\r\n                      >\r\n                        <div class=\"col-md-10 col-8\">\r\n                          <input\r\n                            v-model=\"data.phone\"\r\n                            type=\"text\"\r\n                            class=\"inner form-control\"\r\n                            placeholder=\"Enter your phone no...\"\r\n                          />\r\n                        </div>\r\n                        <div class=\"col-md-2 col-4\">\r\n                          <div class=\"d-grid\">\r\n                            <input\r\n                              type=\"button\"\r\n                              class=\"btn btn-primary btn-block inner\"\r\n                              value=\"Delete\"\r\n                              @click=\"deletePhone(index)\"\r\n                            />\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <input\r\n                      type=\"button\"\r\n                      class=\"btn btn-success inner\"\r\n                      value=\"Add Number\"\r\n                      @click=\"AddPhoneNo\"\r\n                    />\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label class=\"d-block mb-3\">Gender :</label>\r\n\r\n                    <div class=\"form-check form-check-inline\">\r\n                      <input\r\n                        class=\"form-check-input\"\r\n                        type=\"radio\"\r\n                        name=\"gender\"\r\n                        id=\"inlineRadio1\"\r\n                        value=\"option1\"\r\n                      />\r\n                      <label class=\"form-check-label\" for=\"inlineRadio1\"\r\n                        >Male</label\r\n                      >\r\n                    </div>\r\n                    <div class=\"form-check form-check-inline\">\r\n                      <input\r\n                        class=\"form-check-input\"\r\n                        type=\"radio\"\r\n                        name=\"gender\"\r\n                        id=\"inlineRadio2\"\r\n                        value=\"option2\"\r\n                      />\r\n                      <label class=\"form-check-label\" for=\"inlineRadio2\"\r\n                        >Female</label\r\n                      >\r\n                    </div>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"formmessage\">Message :</label>\r\n                    <textarea\r\n                      id=\"formmessage\"\r\n                      class=\"form-control\"\r\n                      rows=\"3\"\r\n                    ></textarea>\r\n                  </div>\r\n                  <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\r\n                </div>\r\n              </div>\r\n            </form>\r\n          </div>\r\n          <!-- end card-body -->\r\n        </div>\r\n        <!-- end card -->\r\n      </div>\r\n      <!-- end col -->\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>"]}]}