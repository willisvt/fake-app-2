k',\r\n          active: true,\r\n        },\r\n      ],\r\n      daterange: '',\r\n      editor: ClassicEditor,\r\n      teamMember: [{ id: 1 }],\r\n    }\r\n  },\r\n  methods: {\r\n    addMember() {\r\n      this.teamMember.push({ name: '' })\r\n    },\r\n    deleteMember(index) {\r\n      this.teamMember.splice(index, 1)\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Create New Task</h4>\r\n            <form class=\"outer-repeater\">\r\n              <div data-repeater-list=\"outer-group\" class=\"outer\">\r\n                <div data-repeater-item class=\"outer\">\r\n                  <div class=\"form-group row mb-4\">\r\n                    <label for=\"taskname\" class=\"col-form-label col-lg-2\">Task Name</label>\r\n                    <div class=\"col-lg-10\">\r\n                      <input\r\n                        id=\"taskname\"\r\n                        name=\"taskname\"\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        placeholder=\"Enter Task Name...\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row mb-4\">\r\n                    <label class=\"col-form-label col-lg-2\">Task Description</label>\r\n                    <div class=\"col-lg-10\">\r\n                      <ckeditor :editor=\"editor\"></ckeditor>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <div class=\"form-group row mb-4\">\r\n                    <label class=\"col-form-label col-lg-2\">Task Date</label>\r\n                    <div class=\"col-lg-10\">\r\n                      <date-picker v-model=\"daterange\" range append-to-body lang=\"en\" confirm></date-picker>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <div class=\"inner-repeater mb-4\">\r\n                    <div class=\"inner form-group mb-0 row\">\r\n                      <label class=\"col-form-label col-lg-2\">Add Team Member</label>\r\n                      <div\r\n                       \r\n                        class=\"inner col-lg-10 ml-md-auto\"\r\n                      >\r\n                        <div class=\"mb-3 row align-items-center\"  v-for=\"( member, index) in teamMember\"\r\n                        :key=\"member.id\">\r\n                          <div class=\"col-md-6\">\r\n                            <input\r\n                              v-model=\"member.name\"\r\n                              type=\"text\"\r\n                              class=\"inner form-control\"\r\n                              placeholder=\"Enter Name...\"\r\n                            />\r\n                          </div>\r\n                          <div class=\"col-md-4\">\r\n                            <div class=\"mt-4 mt-md-0\">\r\n                              <input type=\"file\" class=\"form-control\" />\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"col-md-2\">\r\n                            <div class=\"mt-2 mt-md-0 d-grid\">\r\n                            <input\r\n                              type=\"button\"\r\n                              class=\"btn btn-primary btn-block inner\"\r\n                              value=\"Delete\"\r\n                              @click=\"deleteMember(index)\"\r\n                            />\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"row justify-content-end\">\r\n                      <div class=\"col-lg-10\">\r\n                        <input\r\n                          type=\"button\"\r\n                          class=\"btn btn-success inner\"\r\n                          value=\"Add Number\"\r\n                          @click=\"addMember\"\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row mb-4\">\r\n                    <label for=\"taskbudget\" class=\"col-form-label col-lg-2\">Budget</label>\r\n                    <div class=\"col-lg-10\">\r\n                      <input\r\n                        id=\"taskbudget\"\r\n                        name=\"taskbudget\"\r\n                        type=\"text\"\r\n                        placeholder=\"Enter Task Budget...\"\r\n                        class=\"form-control\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </form>\r\n            <div class=\"row justify-content-end\">\r\n              <div class=\"col-lg-10\">\r\n                <button type=\"submit\" class=\"btn btn-primary\">Create Task</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>\r\n"]}]}