AACA,IAAA,WADA,uBACA,KADA,EACA;AACA,WAAA,UAAA,GAAA,UAAA,CAAA,MAAA,CAAA,UAAA,OAAA,EAAA;AACA,eAAA,OAAA,CAAA,QAAA,IAAA,KAAA,CAAA,YAAA;AACA,OAFA,CAAA;AAGA,KALA;AAOA,IAAA,YAPA,wBAOA,CAPA,EAOA;AACA,UAAA,SAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA;AACA,WAAA,UAAA,GAAA,UAAA,CAAA,MAAA,CAAA,UAAA,OAAA,EAAA;AACA,eACA,OAAA,CAAA,IAAA,CAAA,WAAA,GAAA,MAAA,CAAA,SAAA,CAAA,WAAA,EAAA,MAAA,CAAA,CADA;AAGA,OAJA,CAAA;AAKA,KAdA;AAgBA,IAAA,kBAhBA,8BAgBA,CAhBA,EAgBA,UAhBA,EAgBA;AAAA;;AACA,UAAA,CAAA,KAAA,UAAA,IAAA,KAAA,aAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,aAAA,UAAA,GAAA,UAAA,CAAA,MAAA,CAAA,UAAA,OAAA,EAAA;AACA,iBAAA,OAAA,CAAA,QAAA,GAAA,UAAA;AACA,SAFA,CAAA;AAGA,OAJA,MAIA;AACA,aAAA,UAAA,GAAA,UAAA,CAAA,MAAA,CAAA,UAAA,OAAA,EAAA;AACA,iBAAA,OAAA,CAAA,QAAA,IAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA,EAAA,KAAA,CAAA;AACA,SAFA,EAEA,KAAA,aAFA,CAAA;AAGA;AACA,KA1BA;AA4BA,IAAA,kBA5BA,8BA4BA,CA5BA,EA4BA,UA5BA,EA4BA;AAAA;;AACA,UAAA,CAAA,KAAA,UAAA,EAAA;AACA,aAAA,aAAA,CAAA,IAAA,CAAA,UAAA;AACA,OAFA,MAEA;AACA,aAAA,aAAA,CAAA,MAAA,CAAA,KAAA,aAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA,CAAA;AACA;;AACA,WAAA,UAAA,GAAA,UAAA,CAAA,MAAA,CAAA,UAAA,OAAA,EAAA;AACA,eAAA,OAAA,CAAA,QAAA,IAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA,EAAA,MAAA,CAAA;AACA,OAFA,EAEA,KAAA,aAFA,CAAA;AAGA;AArCA;AApCA,CAAA","sourcesContent":["<script>\r\nimport VueSlideBar from \"vue-slide-bar\";\r\n// import axios from \"axios\";\r\n\r\nimport Layout from \"../../layouts/main\";\r\nimport PageHeader from \"@/components/page-header\";\r\nimport appConfig from \"@/app.config\";\r\n\r\nimport { clothsData } from \"./data-products\";\r\n\r\n/**\r\n * Products component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Product\",\r\n    meta: [{ name: \"description\", content: appConfig.description }],\r\n  },\r\n  components: { VueSlideBar, Layout, PageHeader },\r\n  data() {\r\n    return {\r\n      clothsData: clothsData,\r\n      title: \"Product\",\r\n      items: [\r\n        {\r\n          text: \"Ecommerce\",\r\n          href: \"/\",\r\n        },\r\n        {\r\n          text: \"Products\",\r\n          active: true,\r\n        },\r\n      ],\r\n      sliderPrice: 800,\r\n      currentPage: 1,\r\n      discountRates: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    //** Get data from product api */\r\n    // axios\r\n    //   .get(`http://localhost:8000/api/products`)\r\n    //   .then((res) => {\r\n    //     this.clothsData = res.data.data;\r\n    //   })\r\n    //   .catch((err) => {\r\n    //     return err;\r\n    //   });\r\n  },\r\n  methods: {\r\n    valuechange(value) {\r\n      this.clothsData = clothsData.filter(function (product) {\r\n        return product.newprice <= value.currentValue;\r\n      });\r\n    },\r\n\r\n    searchFilter(e) {\r\n      const searchStr = e.target.value;\r\n      this.clothsData = clothsData.filter((product) => {\r\n        return (\r\n          product.name.toLowerCase().search(searchStr.toLowerCase()) !== -1\r\n        );\r\n      });\r\n    },\r\n\r\n    discountLessFilter(e, percentage) {\r\n      if (e === \"accepted\" && this.discountRates.length === 0) {\r\n        this.clothsData = clothsData.filter((product) => {\r\n          return product.discount < percentage;\r\n        });\r\n      } else {\r\n        this.clothsData = clothsData.filter((product) => {\r\n          return product.discount >= Math.max.apply(null, this);\r\n        }, this.discountRates);\r\n      }\r\n    },\r\n\r\n    discountMoreFilter(e, percentage) {\r\n      if (e === \"accepted\") {\r\n        this.discountRates.push(percentage);\r\n      } else {\r\n        this.discountRates.splice(this.discountRates.indexOf(percentage), 1);\r\n      }\r\n      this.clothsData = clothsData.filter((product) => {\r\n        return product.discount >= Math.max.apply(null, this);\r\n      }, this.discountRates);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-3\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Filter</h4>\r\n\r\n            <div>\r\n              <h5 class=\"font-size-14 mb-3\">Clothes</h5>\r\n              <ul class=\"list-unstyled product-list\">\r\n                <li>\r\n                  <a href=\"javascript: void(0);\">\r\n                    <i class=\"mdi mdi-chevron-right me-1\"></i> T-shirts\r\n                  </a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"javascript: void(0);\">\r\n                    <i class=\"mdi mdi-chevron-right me-1\"></i> Shirts\r\n                  </a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"javascript: void(0);\">\r\n                    <i class=\"mdi mdi-chevron-right me-1\"></i> Jeans\r\n                  </a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"javascript: void(0);\">\r\n                    <i class=\"mdi mdi-chevron-right me-1\"></i> Jackets\r\n                  </a>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"mt-4 pt-3\">\r\n              <h5 class=\"font-size-14 mb-3\">Price</h5>\r\n              <vue-slide-bar\r\n                v-model=\"sliderPrice\"\r\n                :min=\"0\"\r\n                :max=\"1000\"\r\n                @dragEnd=\"valuechange\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"mt-4 pt-3\">\r\n              <h5 class=\"font-size-14 mb-3\">Discount</h5>\r\n\r\n              <b-form-checkbox\r\n                id=\"productdiscountCheck1\"\r\n                class=\"mt-2 form-check\"\r\n                value=\"accepted\"\r\n                unchecked-value=\"not_accepted\"\r\n                @change=\"discountLessFilter($event, 10)\"\r\n                >Less than 10%</b-form-checkbox\r\n              >\r\n\r\n              <b-form-checkbox\r\n                id=\"productdiscountCheck2\"\r\n                class=\"mt-2 form-check\"\r\n                value=\"accepted\"\r\n                unchecked-value=\"not_accepted\"\r\n                @change=\"discountMoreFilter($event, 10)\"\r\n                >10% or more</b-form-checkbox\r\n              >\r\n\r\n              <b-form-checkbox\r\n                id=\"productdiscountCheck3\"\r\n                class=\"mt-2 form-check\"\r\n                value=\"accepted\"\r\n                unchecked-value=\"not_accepted\"\r\n                @change=\"discountMoreFilter($event, 20)\"\r\n                >20% or more</b-form-checkbox\r\n              >\r\n\r\n              <b-form-checkbox\r\n                id=\"productdiscountCheck4\"\r\n                class=\"mt-2 form-check\"\r\n                value=\"accepted\"\r\n                unchecked-value=\"not_accepted\"\r\n                @change=\"discountMoreFilter($event, 30)\"\r\n                >30% or more</b-form-checkbox\r\n              >\r\n\r\n              <b-form-checkbox\r\n                id=\"productdiscountCheck5\"\r\n                class=\"mt-2 form-check\"\r\n                value=\"accepted\"\r\n                unchecked-value=\"not_accepted\"\r\n                @change=\"discountMoreFilter($event, 40)\"\r\n                >40% or more</b-form-checkbox\r\n              >\r\n\r\n              <b-form-checkbox\r\n                id=\"productdiscountCheck6\"\r\n                class=\"mt-2 form-check\"\r\n                value=\"accepted\"\r\n                unchecked-value=\"not_accepted\"\r\n                @change=\"discountMoreFilter($event, 50)\"\r\n                >50% or more</b-form-checkbox\r\n              >\r\n            </div>\r\n\r\n            <div class=\"mt-4 pt-3\">\r\n              <h5 class=\"font-size-14 mb-3\">Customer Rating</h5>\r\n              <div>\r\n                <b-form-checkbox\r\n                  class=\"form-check\"\r\n                  id=\"checkbox-1\"\r\n                  name=\"checkbox-1\"\r\n                  value=\"accepted\"\r\n                  unchecked-value=\"not_accepted\"\r\n                >\r\n                  4\r\n                  <i class=\"bx bx-star text-warning\"></i> & Above\r\n                </b-form-checkbox>\r\n\r\n                <b-form-checkbox\r\n                  id=\"checkbox-2\"\r\n                  class=\"mt-2 form-check\"\r\n                  name=\"checkbox-2\"\r\n                  value=\"accepted\"\r\n                  unchecked-value=\"not_accepted\"\r\n                >\r\n                  3\r\n                  <i class=\"bx bx-star text-warning\"></i> & Above\r\n                </b-form-checkbox>\r\n\r\n                <b-form-checkbox\r\n                  id=\"checkbox-3\"\r\n                  class=\"mt-2 form-check\"\r\n                  name=\"checkbox-3\"\r\n                  value=\"accepted\"\r\n                  unchecked-value=\"not_accepted\"\r\n                >\r\n                  2\r\n                  <i class=\"bx bx-star text-warning\"></i> & Above\r\n                </b-form-checkbox>\r\n                <b-form-checkbox\r\n                  id=\"checkbox-4\"\r\n                  class=\"mt-2 form-check\"\r\n                  name=\"checkbox-4\"\r\n                  value=\"accepted\"\r\n                  unchecked-value=\"not_accepted\"\r\n                >\r\n                  1\r\n                  <i class=\"bx bx-star text-warning\"></i>\r\n                </b-form-checkbox>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-lg-9\">\r\n        <div class=\"row mb-3\">\r\n          <div class=\"col-xl-4 col-sm-6\">\r\n            <div class=\"mt-2\">\r\n              <h5>Clothes</h5>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-lg-8 col-sm-6\">\r\n            <form class=\"mt-4 mt-sm-0 float-sm-end d-flex align-items-center\">\r\n              <div class=\"search-box me-2\">\r\n                <div class=\"position-relative\">\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control border-0\"\r\n                    placeholder=\"Search...\"\r\n                    @input=\"searchFilter($event)\"\r\n                  />\r\n                  <i class=\"bx bx-search-alt search-icon\"></i>\r\n                </div>\r\n              </div>\r\n              <ul class=\"nav nav-pills product-view-nav\">\r\n                <li class=\"nav-item\">\r\n                  <a class=\"nav-link active\" href=\"javascript: void(0);\">\r\n                    <i class=\"bx bx-grid-alt\"></i>\r\n                  </a>\r\n                </li>\r\n                <li class=\"nav-item\">\r\n                  <a class=\"nav-link\" href=\"javascript: void(0);\">\r\n                    <i class=\"bx bx-list-ul\"></i>\r\n                  </a>\r\n                </li>\r\n              </ul>\r\n            </form>\r\n          </div>\r\n        </div>\r\n        <div class=\"row\">\r\n          <div\r\n            v-for=\"data in clothsData\"\r\n            :key=\"data.id\"\r\n            class=\"col-xl-4 col-sm-6\"\r\n          >\r\n            <div class=\"card\">\r\n              <div class=\"card-body\">\r\n                <div class=\"product-img position-relative\">\r\n                  <div v-if=\"data.discount\" class=\"avatar-sm product-ribbon\">\r\n                    <span class=\"avatar-title rounded-circle bg-primary\"\r\n                      >-{{ data.discount }}%</span\r\n                    >\r\n                  </div>\r\n                  <router-link\r\n                    :to=\"`/ecommerce/product-detail/${data.id}`\"\r\n                  >\r\n                    <img\r\n                      :src=\"`${data.product}`\"\r\n                      alt\r\n                      class=\"img-fluid mx-auto d-block\"\r\n                    />\r\n                  </router-link>\r\n                </div>\r\n                <div class=\"mt-4 text-center\">\r\n                  <h5 class=\"mb-3 text-truncate\">\r\n                    <router-link\r\n                      class=\"text-dark\"\r\n                      :to=\"`/ecommerce/product-detail/${data.id}`\"\r\n                      >{{ data.name }}</router-link\r\n                    >\r\n                  </h5>\r\n                  <p class=\"text-muted\">\r\n                    <i class=\"bx bxs-star text-warning\"></i>\r\n                    <i class=\"bx bxs-star text-warning\"></i>\r\n                    <i class=\"bx bxs-star text-warning\"></i>\r\n                    <i class=\"bx bxs-star text-warning\"></i>\r\n                    <i class=\"bx bxs-star\"></i>\r\n                  </p>\r\n                  <h5 class=\"my-0\">\r\n                    <span class=\"text-muted me-2\">\r\n                      <del>${{ data.oldprice }}</del>\r\n                    </span>\r\n                    <b>${{ data.newprice }}</b>\r\n                  </h5>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- end row -->\r\n\r\n        <div class=\"row\">\r\n          <div class=\"col-lg-12\">\r\n            <b-pagination\r\n              v-if=\"clothsData.length > 0\"\r\n              class=\"justify-content-center\"\r\n              pills\r\n              v-model=\"currentPage\"\r\n              :total-rows=\"clothsData.length\"\r\n              :per-page=\"6\"\r\n              aria-controls=\"my-table\"\r\n            ></b-pagination>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>\r\n"],"sourceRoot":"src/router/views/ecommerce"}]}