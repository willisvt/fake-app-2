\n    title: \"General UI\",\r\n    meta: [\r\n      {\r\n        name: \"description\",\r\n        content: appConfig.description,\r\n      },\r\n    ],\r\n  },\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n  },\r\n  data() {\r\n    return {\r\n      title: \"General\",\r\n      items: [\r\n        {\r\n          text: \"UI Elements\",\r\n          href: \"/\",\r\n        },\r\n        {\r\n          text: \"General\",\r\n          active: true,\r\n        },\r\n      ],\r\n      defaultvalue: 3,\r\n      currentpage: 3,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-6\">\r\n                <div>\r\n                  <h4 class=\"card-title\">Badges</h4>\r\n                  <p class=\"card-title-desc\">\r\n                    Add any of the below mentioned modifier classes to change\r\n                    the appearance of a badge.\r\n                  </p>\r\n                  <div>\r\n                    <span class=\"badge bg-primary\">Primary</span>\r\n                    <span class=\"badge bg-success ms-1\">Success</span>\r\n                    <span class=\"badge bg-info ms-1\">Info</span>\r\n                    <span class=\"badge bg-warning ms-1\">Warning</span>\r\n                    <span class=\"badge bg-danger ms-1\">Danger</span>\r\n                    <span class=\"badge bg-dark ms-1\">Dark</span>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-lg-6\">\r\n                <div class=\"mt-4 mt-lg-0\">\r\n                  <h4 class=\"card-title\">Pill Badges</h4>\r\n                  <p class=\"card-title-desc\">\r\n                    Use the\r\n                    <code>.badge-pill</code> modifier class to make badges more\r\n                    rounded (with a larger\r\n                    <code>border-radius</code>\r\n                    and additional horizontal\r\n                    <code>padding</code>). Useful if you miss the badges from\r\n                    v3.\r\n                  </p>\r\n\r\n                  <div>\r\n                    <span class=\"badge rounded-pill bg-primary\">Primary</span>\r\n                    <span class=\"badge rounded-pill bg-success ms-1\"\r\n                      >Success</span\r\n                    >\r\n                    <span class=\"badge rounded-pill bg-info ms-1\">Info</span>\r\n                    <span class=\"badge rounded-pill bg-warning ms-1\"\r\n                      >Warning</span\r\n                    >\r\n                    <span class=\"badge rounded-pill bg-danger ms-1\"\r\n                      >Danger</span\r\n                    >\r\n                    <span class=\"badge rounded-pill bg-dark ms-1\">Dark</span>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!-- end row -->\r\n          </div>\r\n        </div>\r\n        <!-- end card -->\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title\">Popovers</h4>\r\n            <p class=\"card-title-desc\">\r\n              Add small overlay content, like those found in iOS, to any element\r\n              for housing secondary information.\r\n            </p>\r\n\r\n            <div class=\"button-items\">\r\n              <b-button\r\n                v-b-popover.top=\"\r\n                  'Vivamus sagittis lacus vel augue laoreet rutrum faucibus.'\r\n                \"\r\n                variant=\"secondary\"\r\n                >Popover on top</b-button\r\n              >\r\n\r\n              <b-button\r\n                v-b-popover.bottom=\"\r\n                  'Vivamus sagittis lacus vel augue laoreet rutrum faucibus.'\r\n                \"\r\n                variant=\"secondary\"\r\n                >Popover on bottom</b-button\r\n              >\r\n              <b-button\r\n                v-b-popover.right=\"\r\n                  'Vivamus sagittis lacus vel augue laoreet rutrum faucibus.'\r\n                \"\r\n                variant=\"secondary\"\r\n                >Popover on right</b-button\r\n              >\r\n              <b-button\r\n                v-b-popover.left=\"\r\n                  'Vivamus sagittis lacus vel augue laoreet rutrum faucibus.'\r\n                \"\r\n                variant=\"secondary\"\r\n                >Popover on left</b-button\r\n              >\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title\">Tooltips</h4>\r\n            <p class=\"card-title-desc\">\r\n              Hover over the links below to see tooltips:\r\n            </p>\r\n\r\n            <div class=\"button-items\">\r\n              <b-button id=\"tooltip-top\" variant=\"primary\"\r\n                >Tooltip on top</b-button\r\n              >\r\n\r\n              <b-tooltip\r\n                target=\"tooltip-top\"\r\n                placement=\"top\"\r\n                title=\"Tooltip on top\"\r\n              >\r\n              </b-tooltip>\r\n\r\n              <b-button id=\"tooltip-bottom\" variant=\"primary\"\r\n                >Tooltip on bottom</b-button\r\n              >\r\n              <b-tooltip\r\n                target=\"tooltip-bottom\"\r\n                placement=\"bottom\"\r\n                title=\"Tooltip on bottom\"\r\n              >\r\n              </b-tooltip>\r\n\r\n              <b-button id=\"tooltip-left\" variant=\"primary\"\r\n                >Tooltip on left</b-button\r\n              >\r\n              <b-tooltip\r\n                target=\"tooltip-left\"\r\n                placement=\"left\"\r\n                title=\"Tooltip on left\"\r\n              >\r\n              </b-tooltip>\r\n\r\n              <b-button id=\"tooltip-right\" variant=\"primary\"\r\n                >Tooltip on right</b-button\r\n              >\r\n              <b-tooltip\r\n                target=\"tooltip-right\"\r\n                placement=\"right\"\r\n                title=\"Tooltip on right\"\r\n              >\r\n              </b-tooltip>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Pagination</h4>\r\n\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-6\">\r\n                <h5 class=\"font-size-14\">Default Example</h5>\r\n                <p class=\"card-title-desc\">\r\n                  Pagination links indicate a series of related content exists\r\n                  across multiple pages.\r\n                </p>\r\n                <b-pagination\r\n                  v-model=\"defaultvalue\"\r\n                  :total-rows=\"50\"\r\n                  :per-page=\"10\"\r\n                  aria-controls=\"my-table\"\r\n                ></b-pagination>\r\n              </div>\r\n\r\n              <div class=\"col-lg-6\">\r\n                <div class=\"mt-4\">\r\n                  <h5 class=\"card-title\">Alignment</h5>\r\n                  <p class=\"card-title-desc\">\r\n                    Change the alignment of pagination components with flexbox\r\n                    utilities.\r\n                  </p>\r\n\r\n                  <b-pagination\r\n                    :total-rows=\"50\"\r\n                    prev-text=\"Previous\"\r\n                    next-text=\"Next\"\r\n                    align=\"center\"\r\n                  ></b-pagination>\r\n\r\n                  <b-pagination\r\n                    :total-rows=\"50\"\r\n                    prev-text=\"Previous\"\r\n                    next-text=\"Next\"\r\n                    align=\"right\"\r\n                  ></b-pagination>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!-- end row -->\r\n\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-6\">\r\n                <div class=\"mt-4\">\r\n                  <h5 class=\"font-size-14\">Sizing</h5>\r\n                  <p class=\"card-title-desc\">\r\n                    Fancy larger or smaller pagination? Add\r\n                    <code>.pagination-lg</code> or\r\n                    <code>.pagination-sm</code> for additional sizes.\r\n                  </p>\r\n                  <b-pagination\r\n                    size=\"lg\"\r\n                    :total-rows=\"30\"\r\n                    :per-page=\"10\"\r\n                    aria-controls=\"my-table\"\r\n                  ></b-pagination>\r\n\r\n                  <b-pagination\r\n                    size=\"sm\"\r\n                    :total-rows=\"30\"\r\n                    :per-page=\"10\"\r\n                    aria-controls=\"my-table\"\r\n                  ></b-pagination>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!-- end row -->\r\n          </div>\r\n        </div>\r\n        <!-- end card -->\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title\">Border spinner</h4>\r\n            <p class=\"card-title-desc\">\r\n              Use the border spinners for a lightweight loading indicator.\r\n            </p>\r\n            <div>\r\n              <b-spinner\r\n                class=\"m-2\"\r\n                variant=\"primary\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                class=\"m-2\"\r\n                variant=\"secondary\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                class=\"m-2\"\r\n                variant=\"success\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner class=\"m-2\" variant=\"info\" role=\"status\"></b-spinner>\r\n              <b-spinner\r\n                class=\"m-2\"\r\n                variant=\"warning\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner class=\"m-2\" variant=\"danger\" role=\"status\"></b-spinner>\r\n              <b-spinner class=\"m-2\" variant=\"dark\" role=\"status\"></b-spinner>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title\">Growing spinner</h4>\r\n            <p class=\"card-title-desc\">\r\n              If you don’t fancy a border spinner, switch to the grow spinner.\r\n              While it doesn’t technically spin, it does repeatedly grow!\r\n            </p>\r\n            <div>\r\n              <b-spinner\r\n                type=\"grow\"\r\n                class=\"m-2\"\r\n                variant=\"primary\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                type=\"grow\"\r\n                class=\"m-2\"\r\n                variant=\"secondary\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                type=\"grow\"\r\n                class=\"m-2\"\r\n                variant=\"success\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                type=\"grow\"\r\n                class=\"m-2\"\r\n                variant=\"info\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                type=\"grow\"\r\n                class=\"m-2\"\r\n                variant=\"warning\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                type=\"grow\"\r\n                class=\"m-2\"\r\n                variant=\"danger\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n              <b-spinner\r\n                type=\"grow\"\r\n                class=\"m-2\"\r\n                variant=\"dark\"\r\n                role=\"status\"\r\n              ></b-spinner>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>\r\n"],"sourceRoot":"src/router/views/ui"}]}