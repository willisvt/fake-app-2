,KAAA;AAuBA,GAnCA;AAoCA,EAAA,QAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,aAAA,KAAA,SAAA,CAAA,MAAA;AACA;AAHA,GApCA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,SAAA,UAAA,GAAA,CAAA;AACA,SAAA,QAAA,GAAA,KAAA,OAAA;AACA,SAAA,kBAAA,GAAA,KAAA,SAAA,CAAA,KAAA,CACA,KAAA,UADA,EAEA,KAAA,QAFA,CAAA;AAIA,GAhDA;AAiDA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,WAAA,GAAA,CAAA,IAAA,KAAA,OAAA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,WAAA,GAAA,CAAA,IAAA,KAAA,OAAA,GAAA,KAAA,OAAA;AAEA,WAAA,kBAAA,GAAA,KAAA,SAAA,CAAA,KAAA,CACA,KAAA,UADA,EAEA,KAAA,QAFA,CAAA;AAIA;AATA;AAjDA,CAAA","sourcesContent":["<script>\r\nimport Layout from \"../../layouts/main\";\r\nimport PageHeader from \"@/components/page-header\";\r\nimport appConfig from \"@/app.config\";\r\nimport Toolbar from \"./toolbar\";\r\nimport Sidepanel from \"./sidepanel\";\r\n\r\nimport { emailData } from \"./data-inbox\";\r\n\r\n/**\r\n * Email-inbox component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Inbox\",\r\n    meta: [{ name: \"description\", content: appConfig.description }]\r\n  },\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n    Toolbar,\r\n    Sidepanel\r\n  },\r\n  data() {\r\n    return {\r\n      emailData: emailData,\r\n      paginatedEmailData: emailData,\r\n      title: \"Inbox\",\r\n      items: [\r\n        {\r\n          text: \"Email\",\r\n          href: \"/\"\r\n        },\r\n        {\r\n          text: \"Inbox\",\r\n          active: true\r\n        }\r\n      ],\r\n      // page number\r\n      currentPage: 1,\r\n      // default page size\r\n      perPage: 15,\r\n      emailIds: [],\r\n      // start and end index\r\n      startIndex: 1,\r\n      endIndex: 15\r\n    };\r\n  },\r\n  computed: {\r\n    rows() {\r\n      return this.emailData.length;\r\n    }\r\n  },\r\n  created() {\r\n    this.startIndex = 0;\r\n    this.endIndex = this.perPage;\r\n    this.paginatedEmailData = this.emailData.slice(\r\n      this.startIndex,\r\n      this.endIndex\r\n    );\r\n  },\r\n  methods: {\r\n    onPageChange() {\r\n      this.startIndex = (this.currentPage - 1) * this.perPage;\r\n      this.endIndex = (this.currentPage - 1) * this.perPage + this.perPage;\r\n\r\n      this.paginatedEmailData = this.emailData.slice(\r\n        this.startIndex,\r\n        this.endIndex\r\n      );\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    <div class=\"row\">\r\n      <!-- Right Sidebar -->\r\n      <div class=\"col-12\">\r\n        <Sidepanel />\r\n        <div class=\"email-rightbar mb-3\">\r\n          <div class=\"card\">\r\n            <div class=\"btn-toolbar p-3\">\r\n              <Toolbar />\r\n            </div>\r\n            <div class=\"mt-3\">\r\n              <ul class=\"message-list\">\r\n                 <router-link\r\n                 custom v-slot=\"{ navigate }\"\r\n                  v-for=\"(email,index) in paginatedEmailData\"\r\n                  :key=\"index\"\r\n                  :class=\"{ 'unread': email.unread === true }\"\r\n                  :to=\"`/email/reademail/${email.id}`\"\r\n                >\r\n                <li @click=\"navigate\" @keypress.enter=\"navigate\">\r\n                  <div class=\"col-mail col-mail-1\">\r\n                    <div class=\"checkbox-wrapper-mail\">\r\n                      <input :id=\"`chk-${index}`\" type=\"checkbox\" />\r\n                      <label :for=\"`chk-${index}`\"></label>\r\n                    </div>\r\n                    <span :class=\"`star-toggle far fa-star text-${email.text}`\"></span>\r\n                    <a class=\"title\">{{email.title}}</a>\r\n                  </div>\r\n                  <div class=\"col-mail col-mail-2\">\r\n                    <router-link\r\n                      :to=\"`/email/reademail/${email.id}`\"\r\n                      class=\"subject\"\r\n                    >{{email.subject}}</router-link>\r\n                    <div class=\"date\">{{email.date}}</div>\r\n                  </div>\r\n                  </li>\r\n                </router-link>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n          <div class=\"row justify-content-md-between align-items-md-center\">\r\n            <div class=\"col-xl-7\">Showing {{startIndex}} - {{endIndex}} of {{rows}}</div>\r\n            <!-- end col-->\r\n            <div class=\"col-xl-5\">\r\n              <div class=\"text-md-right float-end mt-2 pagination-rounded\">\r\n                <b-pagination\r\n                  v-model=\"currentPage\"\r\n                  :total-rows=\"rows\"\r\n                  :per-page=\"perPage\"\r\n                  @input=\"onPageChange\"\r\n                ></b-pagination>\r\n              </div>\r\n            </div>\r\n            <!-- end col-->\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>"],"sourceRoot":"src/router/views/email"}]}