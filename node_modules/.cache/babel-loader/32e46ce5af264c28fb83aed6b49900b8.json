AA,EAAA,SADA;AAEA,UAAA,MAAA,EAAA;AAFA,SAJA,EAQA;AACA,UAAA,KAAA,EAAA;AADA,SARA,EAWA;AACA,UAAA,KAAA,EAAA,MADA;AAEA,UAAA,MAAA,EAAA;AAFA,SAXA,EAeA;AACA,UAAA,KAAA,EAAA;AADA,SAfA,EAkBA;AACA,UAAA,KAAA,EAAA,cADA;AAEA,UAAA,MAAA,EAAA;AAFA,SAlBA,EAsBA;AACA,UAAA,KAAA,EAAA;AADA,SAtBA,CAHA;AA6BA,QAAA,UAAA,EAAA;AA7BA,OAdA;AA6CA,MAAA,UAAA,EAAA;AACA,QAAA,KAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAFA;AAGA,QAAA,KAAA,EAAA,CACA;AACA,UAAA,KAAA,EAAA;AADA,SADA,EAIA;AACA,UAAA,KAAA,EAAA;AADA,SAJA,EAOA;AACA,UAAA,KAAA,EAAA;AADA,SAPA,EAUA;AACA,UAAA,KAAA,EAAA;AADA,SAVA,EAaA;AACA,UAAA,KAAA,EAAA;AADA,SAbA,EAgBA;AACA,UAAA,KAAA,EAAA;AADA,SAhBA,EAmBA;AACA,UAAA,KAAA,EAAA;AADA,SAnBA;AAHA,OA7CA;AAwEA,MAAA,MAAA,EAAA,IAxEA;AAyEA,MAAA,YAAA,EAAA,CAzEA;AA0EA,MAAA,WAAA,EAAA,GA1EA;AA2EA,MAAA,UAAA,EAAA;AA3EA,KAAA;AA6EA,GApFA;AAqFA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,aAJA,yBAIA,GAJA,EAIA;AACA,WAAA,eAAA,CAAA,UAAA,GAAA,GAAA;AACA,KANA;;AAOA;AACA;AACA;AACA,IAAA,SAVA,uBAUA;AAAA;;AACA,WAAA,MAAA,GAAA,WAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,YAAA;;AACA,YAAA,KAAA,CAAA,YAAA,KAAA,GAAA,EAAA;AACA,UAAA,aAAA,CAAA,KAAA,CAAA,MAAA,CAAA;AACA;AACA,OALA,EAKA,GALA,CAAA;AAMA;AAjBA;AArFA,CAAA","sourcesContent":["<script>\r\nimport VueSlideBar from \"vue-slide-bar\";\r\n\r\nimport Layout from \"../../layouts/main\";\r\nimport PageHeader from \"@/components/page-header\";\r\nimport appConfig from \"@/app.config\";\r\n\r\n/**\r\n * Range-slider component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Range Slider\",\r\n    meta: [{ name: \"description\", content: appConfig.description }]\r\n  },\r\n  components: { VueSlideBar, Layout, PageHeader },\r\n  data() {\r\n    return {\r\n      title: \"Range Slider\",\r\n      items: [\r\n        {\r\n          text: \"UI Elements\",\r\n          href: \"/\"\r\n        },\r\n        {\r\n          text: \"Range Slider\",\r\n          active: true\r\n        }\r\n      ],\r\n      simpleValue: 10,\r\n      sliderCustomzie: 300,\r\n      sliderWithLabel: {\r\n        value: 45,\r\n        data: [15, 30, 45, 60, 75, 90, 120],\r\n        range: [\r\n          {\r\n            label: \"15 mins\"\r\n          },\r\n          {\r\n            label: \"30 mins\",\r\n            isHide: true\r\n          },\r\n          {\r\n            label: \"45 mins\"\r\n          },\r\n          {\r\n            label: \"1 hr\",\r\n            isHide: true\r\n          },\r\n          {\r\n            label: \"1 hr 15 mins\"\r\n          },\r\n          {\r\n            label: \"1 hr 30 mins\",\r\n            isHide: true\r\n          },\r\n          {\r\n            label: \"2 hrs\"\r\n          }\r\n        ],\r\n        rangeValue: {}\r\n      },\r\n      customData: {\r\n        value: 3,\r\n        data: [1, 2, 3, 4, 5, 6, 7],\r\n        range: [\r\n          {\r\n            label: \"Sunday\"\r\n          },\r\n          {\r\n            label: \"Monday\"\r\n          },\r\n          {\r\n            label: \"Tuesday\"\r\n          },\r\n          {\r\n            label: \"Wednesday\"\r\n          },\r\n          {\r\n            label: \"Thursday\"\r\n          },\r\n          {\r\n            label: \"Friday\"\r\n          },\r\n          {\r\n            label: \"Saturday\"\r\n          }\r\n        ]\r\n      },\r\n      loader: null,\r\n      loadingValue: 0,\r\n      customStyle: 800,\r\n      lineHeight: 10\r\n    };\r\n  },\r\n  methods: {\r\n    /**\r\n     * Range and label slider set range\r\n     */\r\n    callbackRange(val) {\r\n      this.sliderWithLabel.rangeValue = val;\r\n    },\r\n    /**\r\n     * Loading slider\r\n     */\r\n    startLoad() {\r\n      this.loader = setInterval(() => {\r\n        this.loadingValue++;\r\n        if (this.loadingValue === 100) {\r\n          clearInterval(this.loader);\r\n        }\r\n      }, 100);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    <div class=\"row\">\r\n      <div class=\"col-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title\">Range slider</h4>\r\n            <p\r\n              class=\"card-title-desc\"\r\n            >Cool, comfortable, responsive and easily customizable range slider</p>\r\n            <div class=\"row\">\r\n              <div class=\"col-md-6\">\r\n                <div class=\"p-3\">\r\n                  <h5 class=\"font-size-14 mb-3 mt-0\">Default</h5>\r\n                  <div class=\"px-3\">\r\n                    <vue-slide-bar v-model=\"simpleValue\" />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-md-6\">\r\n                <div class=\"p-3\">\r\n                  <h5 class=\"font-size-14 mb-3 mt-0\">Min-Max</h5>\r\n                  <div class=\"px-3\">\r\n                    <vue-slide-bar v-model=\"sliderCustomzie\" :min=\"100\" :max=\"500\" />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col-md-6\">\r\n                <div class=\"p-3\">\r\n                  <h5 class=\"font-size-14 mb-3 mt-0\">Range and Label</h5>\r\n                  <div class=\"px-3\">\r\n                    <vue-slide-bar\r\n                      v-model=\"sliderWithLabel.value\"\r\n                      :data=\"sliderWithLabel.data\"\r\n                      :range=\"sliderWithLabel.range\"\r\n                      @callbackRange=\"callbackRange\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-md-6\">\r\n                <div class=\"p-3\">\r\n                  <h5 class=\"font-size-14 mb-3 mt-0\">Loading</h5>\r\n                  <div class=\"px-3\">\r\n                    <vue-slide-bar v-model=\"loadingValue\" :show-tooltip=\"true\" :is-disabled=\"true\" />\r\n                    <br />\r\n                    <b-button size=\"sm\" variant=\"light\" @click=\"startLoad\">Start</b-button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col-md-6\">\r\n                <div class=\"p-3\">\r\n                  <h5 class=\"font-size-14 mb-3 mt-0\">Custom Style</h5>\r\n                  <div class=\"px-3\">\r\n                    <vue-slide-bar\r\n                      v-model=\"customStyle\"\r\n                      :min=\"100\"\r\n                      :max=\"1000\"\r\n                      :line-height=\"lineHeight\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-md-6\">\r\n                <div class=\"p-3\">\r\n                  <h5 class=\"font-size-14 mb-3 mt-0\">Custom Data</h5>\r\n                  <div class=\"px-3\">\r\n                    <vue-slide-bar :data=\"customData.data\" :range=\"customData.range\" />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>"],"sourceRoot":"src/router/views/ui"}]}