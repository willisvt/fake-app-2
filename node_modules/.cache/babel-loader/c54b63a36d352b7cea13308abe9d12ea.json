-group>\r\n\r\n                  <b-form-group\r\n                    class=\"mb-3\"\r\n                    id=\"example-week\"\r\n                    label-cols-sm=\"2\"\r\n                    label-cols-lg=\"2\"\r\n                    label=\"Week\"\r\n                    label-for=\"week\"\r\n                  >\r\n                    <b-form-input\r\n                      id=\"week\"\r\n                      value=\"2019-W33\"\r\n                      type=\"week\"\r\n                    ></b-form-input>\r\n                  </b-form-group>\r\n\r\n                  <b-form-group\r\n                    class=\"mb-3\"\r\n                    id=\"example-time\"\r\n                    label-cols-sm=\"2\"\r\n                    label-cols-lg=\"2\"\r\n                    label=\"Time\"\r\n                    label-for=\"time\"\r\n                  >\r\n                    <b-form-input\r\n                      id=\"time\"\r\n                      value=\"13:45:00\"\r\n                      type=\"time\"\r\n                    ></b-form-input>\r\n                  </b-form-group>\r\n\r\n                  <b-form-group\r\n                    class=\"mb-3\"\r\n                    id=\"example-color\"\r\n                    label-cols-sm=\"2\"\r\n                    label-cols-lg=\"2\"\r\n                    label=\"Color\"\r\n                    label-for=\"color\"\r\n                  >\r\n                    <b-form-input\r\n                      class=\"form-control-color mw-100\"\r\n                      id=\"color\"\r\n                      type=\"color\"\r\n                      name=\"color\"\r\n                      value=\"#1A88F0\"\r\n                    ></b-form-input>\r\n                  </b-form-group>\r\n\r\n                  <div class=\"mb-3 row\">\r\n                    <label class=\"col-md-2 col-form-label\">Select</label>\r\n                    <div class=\"col-md-10\">\r\n                      <select class=\"form-control\">\r\n                    